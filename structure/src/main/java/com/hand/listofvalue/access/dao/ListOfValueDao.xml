<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hand.listofvalue.access.dao.ListOfValueDao">
    <select id="toUniqueVerify" parameterType="com.hand.listofvalue.access.vo.ListOfValueVO" resultType="int">
        SELECT COUNT(*) FROM HC_LST_OF_VAL WHERE lang_id=#{langId} AND type=#{type} AND (name=#{name} OR val=#{val})
    </select>
    <insert id="insertLstOfVal" parameterType="com.hand.listofvalue.access.vo.ListOfValueVO" useGeneratedKeys="true" keyProperty="code">
        INSERT INTO
         HC_LST_OF_VAL(CODE,CREATED,CREATED_BY,UPDATED,UPDATED_BY,TYPE,LANG_ID,VAL,NAME,PAR_LST_CODE,LEVEL,ACT_FLG,DESC)
         VALUES (#{code},#{created},#{createdBy},#{updated},#{updatedBy},#{type},#{langId},#{val},#{name},#{parLstCode},#{level},#{actFlg},#{desc})
    </insert>
</mapper>